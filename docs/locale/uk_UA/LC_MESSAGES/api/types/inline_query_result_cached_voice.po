#: of
msgid "Type of the result, must be *voice*"
msgstr ""

#: ../../docstring
#: aiogram.types.inline_query_result_cached_voice.InlineQueryResultCachedVoice.id:1
#: of
msgid "Unique identifier for this result, 1-64 bytes"
msgstr ""

#: ../../docstring
#: aiogram.types.inline_query_result_cached_voice.InlineQueryResultCachedVoice.voice_file_id:1
#: of
msgid "A valid file identifier for the voice message"
msgstr ""

